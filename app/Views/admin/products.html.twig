{% extends template %}
{% block main %}

{% import "components/components.html.twig" as components %}

<div class="flex flex-col w-full h-full">
  <header class="flex justify-between py-4 px-7">
    <h1 class="text-3xl font-bold">Daftar Produk</h1>
    {% if products %}
    <a class="flex justify-center" href="{{ route_to('admin/add-product') }}">
      <button class="btn btn-base btn-black">
        <svg><use xlink:href="#ph_plus-bold"></svg>
        <span>Tambah produk</span>
      </button>
    </a>
    {% endif %}
  </header>
    
  {% if products %}
    <div class="flex items-center w-full gap-4 px-7 mt-7">
      <template x-data="{
        selected: 0,
        chips: [
          { text: 'Semua produk ({{ products_length }})' },
          { text: 'Stok ada' },
          { text: 'Stok habis' },
          { text: 'Draf' },
        ]
      }"
      x-for="(chip, index) in chips">
        <button :selected="selected === index" @click="selected = index" class="chip chip-base">
          <span x-text="chip.text" :class="{ 'font-semibold': selected === index }"></span>
        </button>
      </template>
      <input type="text" name="search" placeholder="Cari produk" autocomplete class="flex-grow input input-base input-default">
    </div>
    <div class="px-7 mt-9">
      <table class="text-left table-fixed">
        <thead class="text-lg text-trueGray-500">
          <tr class="mb-3">
            <th class="w-12 pb-4">
              <svg class="inline w-6 h-6 mr-6 fill-current"><use xlink:href="#ph_square"></svg>
            </th>
            <th class="pb-4 font-semibold w-96">Produk</th>
            <th class="pb-4 font-semibold w-52">Harga</th>
            <th class="pb-4 font-semibold w-36">Stok</th>
            <th></th>
          </tr>
        </thead>
        <tbody class="border-t border-b divide-y divide-trueGray-100 border-trueGray-100">
          {% for product in products %}
          <tr class="product-{{ product.id }} group hover:bg-trueGray-200">
            <td class="h-20">
              <svg class="w-6 h-6 fill-current"><use xlink:href="#ph_square"></svg>
            </td>
            <td class="space-x-6">
              <img src="{{ route_to('content-photos', 'sm', product.filenames[0].filename) }}" class="inline-block w-16 h-16 rounded-lg bg-trueGray-300">
              <span class="w-64">{{ product.title }}</span>
            </td>
            <td>
              <span>Rp</span>
              <span>{{ product.price }}</span>
            </td>
            <td>{{ 
              components.switch({
                name: "stock",
                value: product.stock == 1 ? 0 : 1,
                checked: product.stock == 1,
                activeText: 'Ada',
                inactiveText: 'Habis',
                attributes: 'hx-put="' ~ route_to('update-stock', product.id) ~ '" hx-include="' ~ ".product-#{ product.id } [name='stock']" ~ '" hx-target="#main" hx-indicator="#main-indicator"',
              }) }}
            </td>
            <td class="space-x-6">
              <button disabled class="btn">Lihat statistik</button>
              <button @click="$el.nextElementSibling.dispatchEvent(new CustomEvent('toggle'))" class="opacity-0 group-hover:opacity-100">
                <svg class="inline w-6 h-6 fill-current text-trueGray-700">
                  <use xlink:href="#ph_dots-three-vertical-bold">
                </svg>
              </button>
              {{ components.dropdown({
                selections: [
                  { text: 'Edit' },
                  { text: 'Link untuk share' },
                  { text: 'Tambah varian', attributes: 'disabled' },
                  { text: 'Hapus permanen', icon: 'trash-simple', attributes: 'hx-delete="' ~ route_to('delete-product', product.id) ~ '" hx-confirm="Anda yakin ingin menghapus produk ini?" hx-target="#main" hx-indicator="#main-indicator"' , dangerTheme: true },
                ]
              }) }}
            </td>
          </tr>
          {% endfor %}
        </tbody>
      </table>
    </div>
  {% else %}
    <section class="grid flex-grow w-full place-content-center">
      <div class="flex flex-col gap-6 mx-auto w-96">
